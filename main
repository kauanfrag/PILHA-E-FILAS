class Elemento {
    String id;
    String descricao;
    String info;
    public Elemento(String id, String descricao, String info) {
    this.id = id;
    this.descricao = descricao;
    this.info = info;
}}
class Node {
    Elemento elemento;
    Node prox;
    public Node(Elemento elemento) {
        this.elemento = elemento;
        this.prox = null;
    }
}

class Pilha {
    private Node topo;

    public Pilha() {
        topo = null;
    }
  public void push(Elemento e) {
        Node novo = new Node(e);
        novo.prox = topo;
        topo = novo;
    }
 public Elemento pop() {
        if (topo == null) return null;
        Elemento ret = topo.elemento;
        topo = topo.prox;
        return ret;
    }
   public boolean estaVazia() {
        return topo == null;
    }
