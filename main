class Elemento {
    String id;
    String descricao;
    String info;
    public Elemento(String id, String descricao, String info) {
    this.id = id;
    this.descricao = descricao;
    this.info = info;
}}
class Node {
    Elemento elemento;
    Node prox;
    public Node(Elemento elemento) {
        this.elemento = elemento;
        this.prox = null;
    }
}

